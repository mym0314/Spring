<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="springHelloWorld" class="com.mym.imp.SpringHelloWorld"/>
    <bean id="strustHelloWorld" class="com.mym.imp.StrustHelloWorld"/>

    <bean id="helloWorldService" class="com.mym.services.HelloWorldService">
        <property name="helloWorld" ref="strustHelloWorld"/>
    </bean>
</beans>